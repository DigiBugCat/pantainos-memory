{
  "name": "pantainos-memory",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --experimental-vectorize-bind-to-prod",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:staging": "PANTAINOS_URL=https://staging-pantainos-memory.nekokeys.com vitest run --config vitest.integration.config.ts",
    "test:integration:local": "PANTAINOS_URL=http://localhost:8794 vitest run --config vitest.integration.config.ts"
  },
  "dependencies": {
    "@cloudflare/workers-oauth-provider": "^0.2.2",
    "hono": "^4.6.14",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@vitest/coverage-v8": "^4.0.17",
    "msw": "^2.12.7",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17",
    "wrangler": "^3.99.0"
  }
}
